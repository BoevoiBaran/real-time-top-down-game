{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "GameRoomsServerSettings": {
    "Port": 8008,
    "TickPeriod": 5,
    "PingPeriod": 0,
    "UseStats": true,
    "MaxMessageSize": 16384,
    "MaxMessagesPerTick": 256
  }
}
